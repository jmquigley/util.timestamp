{
	"name": "util.timestamp",
	"version": "0.0.44",
	"author": "jmquigley <jmquigley@outlook.com>",
	"description": "Generates a string timestamp",
	"private": false,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/jmquigley/util.timestamp"
	},
	"bugs": {
		"url": "https://github.com/jmquigley/util.timestamp/issues"
	},
	"engines": {
		"node": ">=7.x"
	},
	"prettier": "@jmquigley/prettier-config",
	"bin": {
		"timestamp": "./cli.js"
	},
	"main": "./index.umd.min.js",
	"module": "./index.es.min.js",
	"types": "./index.d.ts",
	"files": [
		"cli.js",
		"index.js",
		"index.*.min.js",
		"index.d.ts"
	],
	"dependencies": {
		"strftime": "^0.10.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.7.5",
		"@babel/core": "^7.7.5",
		"@babel/plugin-transform-modules-umd": "^7.7.4",
		"@babel/preset-env": "^7.7.6",
		"@babel/preset-typescript": "^7.7.4",
		"@babel/runtime": "^7.7.6",
		"@types/jest": "^24.0.23",
		"@types/node": "^12.12.17",
		"@types/strftime": "^0.9.2",
		"babel-minify": "^0.5.1",
		"babel-preset-minify": "^0.5.1",
		"coveralls": "^3.0.9",
		"jest": "^24.9.0",
		"tslint": "^5.20.1",
		"tslint-config-prettier": "^1.18.0",
		"typescript": "^3.7.3",
		"util.pkgwrap": "^0.0.115"
	},
	"pkgwrap": {
		"cleanup": [
			"cli.d.ts",
			"cli.js",
			"cli.js.map"
		]
	},
	"scripts": {
		"all": "yarn install && yarn build && yarn test && yarn manifest",
		"build": "yarn clean && yarn lint && yarn prettier && yarn build:es && yarn build:umd && yarn build:cli",
		"build:es": "tsc -p . && minify index.js --out-file index.es.min.js",
		"build:umd": "babel index.js --out-file index.umd.min.js",
		"build:cli": "babel cli.js --out-file cli.js",
		"clean": "pkgwrap --clean",
		"clean:dist": "yarn clean && rimraf node_modules/ yarn.lock",
		"coverage": "pkgwrap --coverage --jest",
		"lint": "pkgwrap --lint",
		"manifest": "npm pack --dry-run",
		"prettier": "pkgwrap --prettier",
		"test": "jest"
	}
}
